{
  "name": "earthquake-crawler",
  "version": "1.0.0",
  "description": "Modular earthquake data crawler for Kandilli Observatory with filtering and reporting capabilities",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx src/app.ts --debug",
    "dev:watch": "tsx watch src/app.ts --debug",
    "test-connectivity": "tsx src/app.ts --test-connectivity",
    "no-filter": "tsx src/app.ts --no-filter",
    "json-only": "tsx src/app.ts --no-markdown",
    "markdown-only": "tsx src/app.ts --no-json",
    "performance-test": "tsx src/performance-test.ts",
    "performance-test:build": "npm run build && node dist/performance-test.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "earthquake",
    "kandilli",
    "crawler",
    "data-processing",
    "izmir",
    "turkey",
    "typescript"
  ],
  "author": "Görkem Özkan",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gorkemozkan/earthquake-crawler"
  },
  "bugs": {
    "url": "https://github.com/gorkemozkan/earthquake-crawler/issues"
  }
}
